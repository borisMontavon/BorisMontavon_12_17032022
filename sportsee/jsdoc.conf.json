{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": ["plugins/markdown", "better-docs/component"],
    "templates": {
      "better-docs": {
        "name": "SportSee Jsdoc"
      },
      "cleverLinks": true
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/better-docs"
    }
  }